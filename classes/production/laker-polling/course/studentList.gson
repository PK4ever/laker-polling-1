import edu.oswego.cs.lakerpolling.domains.AuthToken

response.status OK.value()
model {
    AuthToken token
    Long courseID
    Collection<User> students = []
}

json {
    status "success"
    data {
        accessToken token.accessToken
        course_id courseID
        students g.render(template: "../templates/students", model: [students:students])
    }
}